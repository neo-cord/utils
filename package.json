{
  "name": "@neocord/utils",
  "version": "1.0.13",
  "description": "Utilities to use within the NeoCord library.",
  "author": "MeLike2D",
  "homepage": "https://github.com/neo-cord/utils#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo-cord/utils.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --removeComments",
    "build:types": "tsc -d",
    "lint": "eslint . --ext .ts",
    "bundle:types": "dts-bundle --name @neocord/utils --main ./dist --removeSource --outputAsModuleFolder --baseDir ./dist --out ../typings/index.d.ts"
  },
  "bugs": {
    "url": "https://github.com/neo-cord/utils/issues"
  },
  "devDependencies": {
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=8.x.x"
  },
  "dependencies": {
    "tslib": "^2.0.1"
  }
}
